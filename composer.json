{
    "name": "benbanfa/yii2-log",
    "description": "benbanfa/yii2-log",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "benbanfa/exclude-composer-assets": "dev-master",
        "yiisoft/yii2": "^2.0",
        "fluent/logger": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vlucas/phpdotenv": "^3"
    },
    "autoload": {
        "psr-4": {
            "zii\\log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zii\\log\\tests\\": "tests/"
        }
    },
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/benbanfa/exclude-composer-assets.git"
        }
    ]
}
